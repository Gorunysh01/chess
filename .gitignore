
import math
def proverka(n):
    result = True
    for i in range(1, int(math.sqrt(n))):
        if n % i == 0:
            result = False
            break
    return result

def gg(a1,a2):
    prost = a2 // math.gcd(a1, a2)
    if not proverka(prost): 
        for i in range(1, prost // 2):
            if prost % i == 0:
                prost = prost // i
                
    return prost
    

    

